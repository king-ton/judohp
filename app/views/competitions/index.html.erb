<% provide(:title, t('views.competition.index.title')) %>

<table class="table table-striped tablesorter">
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.competition.title') %></th>
      <th><%= t('activerecord.attributes.competition.start_date') %></th>
      <th><%= t('activerecord.attributes.competition.end_date') %></th>
      <th><%= t('views.venue', count: 1) %></th>
      <th><%= t('competition_templates', count: 1) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="competition-index">
  	<%= render 'index'%>
  </tbody>
</table>
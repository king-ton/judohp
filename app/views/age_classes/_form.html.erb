<%= simple_form_for(@age_class) do |f| %>
<div class="row" >
	<div class="col-md-4">
		<%= f.input :name %>
	</div>
</div>
<div class="row" >
	<div class="col-md-2">
		<%= f.input :min_age %>
	</div>
	<div class="col-md-2">
		<%= f.input :max_age %>
	</div>
</div>

<%= f.submit :class => "btn btn-success" %>

<% end %>
